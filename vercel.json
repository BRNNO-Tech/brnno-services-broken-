{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/bundle.js",
      "dest": "/bundle.js"
    },
    {
      "src": "/bundle.js.LICENSE.txt",
      "dest": "/bundle.js.LICENSE.txt"
    },
    {
      "src": "/BRNNO_logov3.jpg",
      "dest": "/BRNNO_logov3.jpg"
    },
    {
      "src": "/app.html",
      "dest": "/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}