{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/bundle.js",
      "dest": "/bundle.js"
    },
    {
      "src": "/bundle.js.LICENSE.txt",
      "dest": "/bundle.js.LICENSE.txt"
    },
    {
      "src": "/BRNNO_logov3.jpg",
      "dest": "/BRNNO_logov3.jpg"
    },
    {
      "src": "/app.html",
      "dest": "/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}